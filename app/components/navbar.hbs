<div local-class="root">
  <NavbarIcon
    @icon="bars"
    @isExpanded={{this.isExpanded}}
    type="button"
    {{on "click" this.toggleIsExpanded}}
  />
  <NavbarIcon @icon="dove" @text="Apps" @route="protected.admin-apps" @isExpanded={{this.isExpanded}} />
  <NavbarIcon @icon="user-friends" @text="Users" @route="protected.admin-users" @isExpanded={{this.isExpanded}} />
  <NavbarIcon @icon="cog" @text="System" @route="protected.admin-system" @isExpanded={{this.isExpanded}} />
  
  <NavbarIcon @icon="user-lock" @text="User Claims" @route="protected.admin-system" @isExpanded={{this.isExpanded}} />
</div>